@using RunGroup.ViewModels;
@model EditUserDashboardViewodel;
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<form asp-action="EditUserprofile" method="post" enctype="multipart/form-data">
    <input type="hidden" id="Id" name="Id" asp-for="Id"/>
        <div class="form-group">
            <label asp-for="City">City:</label>
            <input asp-for="City" type="text" class="form-control form-control-lg" />
            <span asp-validation-for="City" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="State">State:</label>
            <input asp-for="State" type="text" class="form-control form-control-lg" />
            <span asp-validation-for="State" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Pace">Pace:</label>
            <input asp-for="Pace" type="text" class="form-control form-control-lg" />
            <span asp-validation-for="Pace" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="Milage">Mileage:</label>
            <input asp-for="Milage" type="text" class="form-control form-control-lg" />
            <span asp-validation-for="Milage" class="text-danger"></span>
        </div>
    <div class="form-group">
        <label asp-for="Image">Image</label>
        <input asp-for="Image" type="file" class="form-control" id="exampleInputImage" placeholder="Image">
        <span asp-validation-for="Image" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>